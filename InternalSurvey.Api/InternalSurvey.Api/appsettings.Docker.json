{
  "ConnectionString": {
    "SurveyCS": "Server=db;Database=SurveyDb;User Id=sa;Password=Test2020*"
  },
  "Serilog": {
    "Using": [],
    "LevelSwitches": { "$controlSwitch": "Information" },
    "MinimumLevel": { "ControlledBy": "$controlSwitch" },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/var/logs/log.txt",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "outputTemplate": "{Timestamp} {Message}{NewLine:1}{Exception:1}"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThreadId"
    ],
    "Properties": {
      "ApplicationName": "InternalSurvey.Api"
    }
  },
  "ApplicationsUrl": {
    "UrlQuiz": "https://localhost:4202/user-quiz",
    "UrlThankYou": "https://localhost:4202/thankyou",
    "UrlApp": "https://localhost:44316"
  }
}
